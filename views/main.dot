<!DOCTYPE html>
<html>
  <head>
    <style>
      {{=it.inlinestyle}}
    </style>
    <script>
      app = {};
      var bs = {{=it.bs}};
      (function() {
        var link = document.createElement("link");
        link.type = "text/css";
        link.rel = "stylesheet";
        link.href = '{{=it.assetcss}}';
        document.getElementsByTagName("head")[0].appendChild(link);
      })();
      (function(){
        var po = document.createElement('script');
        po.async = true;
        po.src = '{{=it.assetjs}}';
        po.type = 'text/javascript';
        document.getElementsByTagName("head")[0].appendChild(po);
      })();
    </script>
  </head>
  <body>
    <p> caption: {{=it.caption}} </p>
  </body>
</html>


